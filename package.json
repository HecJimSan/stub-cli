{
  "name": "@hectorjs/stub-cli",
  "version": "0.0.1",
  "description": "Command line for stub-backend library",
  "main": "index.js",
  "bin": {
    "hjs": "bin/hjs"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --recursive --require esm --exit",
    "coverage": "nyc --reporter=html --reporter=text mocha --recursive --require esm --exit"
  },
  "keywords": [
    "cli",
    "stub",
    "hectorjs",
    "stub-backend"
  ],
  "author": "hectorjs",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.2",
    "esm": "^3.2.25",
    "minimist": "^1.2.0",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "supertest": "^3.4.2"
  }
}
